.search-results.search-list
  %ul#swipe-gallery
    - @search_results.each do |item|
      %li
        %h2= item.name('en')
        %p
          = item.content
        - item.images.each do |image|
          %a{:href => image.url}
            %img{:src => image.thumbnail_square_url, :title => image.title, :alt => item.name('en')}
